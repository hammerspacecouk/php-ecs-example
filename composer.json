    "symfony/dotenv": "^3.3"
  },
  "authors": [
    {
      "name": "Hammerspace.co.uk",
      "email": "contact@hammerspace.co.uk"
    }
  ],
  "scripts": {
    "test": "phpunit",
    "cs": "phpcs src tests public"
  }
